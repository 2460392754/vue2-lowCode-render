{
    "vue": "2",
    "editorVersion": "1",

    "node": {
        "type": "ElCard",
        "attribute": {
            "style": {
                "margin": "20px",
                "padding": "20px"
            }
        },
        "children": [
            {
                "type": "ElForm",
                "attribute": {
                    "ref": "{{data.formRef}}",
                    "props": {
                        "labelWidth": "100px"
                    }
                },
                "children": [
                    {
                        "type": "ElFormItem",
                        "attribute": {
                            "props": {
                                "prop": "name",
                                "label": "姓名"
                            }
                        },
                        "children": [
                            {
                                "type": "ElInput",
                                "attribute": {
                                    "props": {}
                                },
                                "children": []
                            }
                        ]
                    },
                    {
                        "type": "ElFormItem",
                        "attribute": {},
                        "children": [
                            {
                                "type": "ElButton",
                                "attribute": {
                                    "props": {
                                        "type": "primary"
                                    },
                                    "on": {
                                        "click": "{{methods.onSubmit}}"
                                    }
                                },
                                "children": "提交"
                            }
                        ]
                    }
                ]
            }
        ]
    },

    "data": {
        "formRef": "eval(window.$ref())",
        "formModel": {
            "name": ""
        }
    },

    "methods": {
        "onSubmit": {
            "func": "console.log({{data.formRef}}.value)"
        }
    }
}
